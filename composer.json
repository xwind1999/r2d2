{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "^7.4",
        "doctrine/annotations": "^1.8",
        "gedmo/doctrine-extensions": "^2.4",
        "globtec/phpadr": "dev-master",
        "jms/serializer-bundle": "^3.5",
        "joaojacome/clogger-php": "0.2.0",
        "laminas/laminas-diagnostics": "^1.6",
        "league/csv": "^9.6",
        "liip/monitor-bundle": "^2.12",
        "moneyphp/money": "^3.3",
        "nelmio/api-doc-bundle": "^3.6",
        "ramsey/uuid": "^3.9",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/asset": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/finder": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-client": "5.0.*",
        "symfony/messenger": "5.0.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "infection/infection": "^0.15.0",
        "phpmetrics/phpmetrics": "^2.5",
        "phpstan/phpstan": "^0.12.7",
        "phpunit/phpunit": "^8.5.1",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Psr\\Log\\": "src-vendor-fixes/Psr/Log",
            "Doctrine\\ORM\\": "src-vendor-fixes/doctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "R2D2\\Hooks\\InstallHooks::checkHooks"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
